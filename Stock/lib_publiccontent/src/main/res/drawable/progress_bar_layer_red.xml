<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"> 
     
    <item android:id="@android:id/background"> 
        <shape> 
            <corners android:radius="5dip" /> 
            <gradient 
                    android:startColor="@color/txt_tint_gray" 
                    android:centerColor="@color/txt_tint_gray" 
                    android:centerY="0.75" 
                    android:endColor="@color/txt_tint_gray" 
                    android:angle="270" 
            /> 
        </shape> 
    </item> 
     
    <item android:id="@android:id/secondaryProgress"> 
        <clip> 
            <shape> 
                <corners android:radius="5dip" /> 
                <gradient 
                        android:startColor="@color/txt_tint_gray" 
                        android:centerColor="@color/txt_tint_gray" 
                        android:centerY="0.75" 
                        android:endColor="@color/txt_tint_gray" 
                        android:angle="270" 
                /> 
            </shape> 
        </clip> 
    </item> 
     
    <item android:id="@android:id/progress"> 
        <clip> 
            <shape> 
                <corners android:radius="5dip" /> 
                <gradient 
                        android:startColor="@color/stock_red" 
                        android:centerColor="@color/stock_red" 
                        android:centerY="0.75" 
                        android:endColor="@color/stock_red" 
                        android:angle="270" 
                /> 
            </shape> 
        </clip> 
    </item> 
     
</layer-list> 
