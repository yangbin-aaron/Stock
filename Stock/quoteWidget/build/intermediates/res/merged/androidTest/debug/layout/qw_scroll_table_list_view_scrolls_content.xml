<?xml version="1.0" encoding="utf-8"?>
<com.hundsun.quote.widget.QwScrollTableScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scroll_table_scroll_view"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:layout_weight="1"
    android:background="@color/common_hsv_bg" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:id="@+id/scroll_table_fixed_content_ll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" />

        <com.hundsun.quote.widget.QwScrollTableHorizontalScrollView
            android:id="@+id/scroll_table_moveable_content_hsv"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:fadingEdge="none"
            android:scrollbars="none" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <LinearLayout
                    android:id="@+id/scroll_table_moveable_content_ll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" />
            </LinearLayout>
        </com.hundsun.quote.widget.QwScrollTableHorizontalScrollView>
    </LinearLayout>

</com.hundsun.quote.widget.QwScrollTableScrollView>